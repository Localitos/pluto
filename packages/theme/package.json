{
  "name": "@localyze-pluto/theme",
  "version": "40.0.0",
  "description": "Pluto Design System Theme ",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Localitos/pluto.git",
    "directory": "packages/theme"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react --minify",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "prettier-check": "prettier --check package.json tsconfig.json \"./src/**/*.{tsx,ts,json}\"",
    "test": "jest",
    "tsc": "tsc"
  },
  "peerDependencies": {
    "@localyze-pluto/design-tokens": "^2.0.0",
    "@xstyled/styled-components": "^3.8.0",
    "react": "^17.0.2 || ^18.2.0",
    "react-dom": "^17.0.2 || ^18.2.0",
    "styled-components": "^5.3.11"
  },
  "devDependencies": {
    "@localyze-pluto/design-tokens": "^2.0.0",
    "@localyze-pluto/eslint-config": "^1.23.0",
    "@localyze-pluto/tsconfig": "^1.1.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/styled-components": "^5.1.34",
    "@xstyled/styled-components": "^3.8.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.12",
    "ts-jest": "^29.2.3",
    "tslib": "^2.6.3",
    "tsup": "^8.2.3",
    "typescript": "^5.6.2"
  }
}
