{
  "name": "@localyze-pluto/components",
  "version": "2.3.1",
  "description": "Pluto Design System Components",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Localitos/pluto.git",
    "directory": "packages/components"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --external react --minify",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts --external react",
    "lint": "TIMING=1 eslint src/**/*.ts* --fix",
    "prettier-check": "prettier --check package.json tsconfig.json \"./src/**/*.{tsx,ts,json}\"",
    "test": "jest",
    "tsc": "tsc"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.12",
    "@localyze-pluto/theme": "^1.0.5",
    "@xstyled/styled-components": "^3.6.0",
    "lodash": "^4.17.21",
    "styled-components": "^5.3.6"
  },
  "peerDependencies": {
    "@heroicons/react": "^2.0.12",
    "@localyze-pluto/theme": "^1.0.5",
    "@xstyled/styled-components": "^3.6.0",
    "lodash": "^4.17.21",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@localyze-pluto/eslint-config": "^1.0.1",
    "@localyze-pluto/tsconfig": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "@types/styled-components": "^5.1.26",
    "eslint": "^8.25.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^29.2.1",
    "jest-watch-typeahead": "^2.2.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^28.0.5",
    "tslib": "^2.4.0",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "browserslist": [
    "extends @localyze-pluto/browserslist-config"
  ]
}
