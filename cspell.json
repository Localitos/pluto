{
  "version": "0.2",
  "language": "en",
  "words": ["localyze", "localitos"],
  "ignoreWords": [
    "eamodio",
    "dbaeumer",
    "esbenp",
    "preactjs",
    "sonarjs",
    "tsup",
    "moderat",
    "npmrc",
    "rgba",
    "testid",
    "keyof",
    "toshimaru",
    "webp",
    "chromaui",
    "xstyled",
    "nums",
    "bezier",
    "xlarge",
    "xsmall",
    "unhover",
    "uppy",
    "formik"
  ],
  "flagWords": ["hte"],
  "allowCompoundWords": true,
  "ignorePaths": [
    "**/node_modules/**",
    "**/dist/**",
    "*.svg",
    "*.lock",
    "cspell.json",
    ".gitignore",
    "CHANGELOG.md",
    "CODEOWNERS",
    ".prettierignore"
  ],
  "overrides": [
    {
      "filename": "**/{stories,spec,test}.{ts,tsx,js,jsx}",
      "dictionaries": ["lorem-ipsum"]
    }
  ]
}
