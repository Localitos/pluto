{
  "name": "pluto",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Localitos/pluto.git"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "add-component": "plop",
    "build": "turbo build",
    "build-storybook": "build-storybook",
    "changeset": "changeset",
    "clean": "turbo clean && rm -rf node_modules",
    "dev": "turbo dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "prepare": "husky install",
    "release": "turbo build && changeset publish",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "jest --runInBand --coverage",
    "type-check": "turbo tsc"
  },
  "workspaces": [
    "packages/**"
  ],
  "devDependencies": {
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.14.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.4",
    "cspell": "^6.8.1",
    "danger": "^11.1.4",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.1.2",
    "jest-watch-typeahead": "^2.2.0",
    "lint-staged": "^13.0.3",
    "plop": "^3.1.1",
    "prettier": "^2.5.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "ts-jest": "^28.0.5",
    "turbo": "latest"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "extends @localyze-pluto/browserslist-config"
  ],
  "bugs": {
    "url": "https://github.com/Localitos/pluto/issues"
  },
  "readme": "ERROR: No README data found!",
  "homepage": "https://github.com/Localitos/pluto#readme",
  "_id": "pluto@1.0.0"
}
