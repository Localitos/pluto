{
  "name": "pluto",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Localitos/pluto.git"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "add-component": "plop",
    "build": "turbo build",
    "build-storybook": "build-storybook",
    "changeset": "changeset",
    "clean": "turbo clean && rm -rf node_modules",
    "dev": "turbo dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "prepare": "husky install",
    "release": "turbo build && changeset publish",
    "start": "start-storybook -p 6006",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:coverage": "jest --runInBand --coverage",
    "type-check": "turbo tsc"
  },
  "workspaces": [
    "packages/**"
  ],
  "dependencies": {
    "@tanstack/react-table": "^8.7.3",
    "formik": "^2.2.9",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.40.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@changesets/changelog-github": "0.4.7",
    "@changesets/cli": "^2.25.2",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-docs": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-viewport": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.14",
    "@storybook/manager-webpack5": "^6.5.14",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.4",
    "assert": "^2.0.0",
    "cspell": "^6.17.0",
    "danger": "^11.1.4",
    "eslint": "^8.29.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^29.2.2",
    "jest-watch-typeahead": "^2.2.1",
    "lint-staged": "^13.1.0",
    "plop": "^3.1.1",
    "prettier": "^2.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-jest": "^28.0.5",
    "turbo": "latest"
  },
  "resolutions": {
    "webpack": "^5"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": [
    "extends @localyze-pluto/browserslist-config"
  ],
